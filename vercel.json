{
  "version": 2,
  "builds": [
    {
      "src": "web/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "web/index-new.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "web/index-new.js"
    },
    {
      "src": "/(.*)",
      "dest": "web/index-new.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "web/index-new.js": {
      "maxDuration": 30
    }
  }
}
